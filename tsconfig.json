{
    "extends": "./.nuxt/tsconfig.json",
    "compilerOptions": {
        "moduleResolution": "bundler",
        "jsx": "preserve",
        "noImplicitAny": false,
        "allowJs": true,
        "strict": false,
        "strictNullChecks": false,
        // 아래 types 항목을 추가하세요
        "types": ["@nuxtjs/supabase"]
    },
    "include": [".nuxt/nuxt.d.ts", "**/*"],
    "exclude": ["node_modules", ".output", "dist"]
}
